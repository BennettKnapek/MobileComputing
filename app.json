    "name": "MobileComputing",
    "slug": "MobileComputing",
    "platforms": [
      "android",
      "web"
    ],
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
